<p id="notice"><%= notice %></p>

<h1>Tag Associations</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Created at</th>
      <th>Tagable</th>
      <th>Tag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tag_associations.each do |tag_association| %>
      <tr>
        <td><%= tag_association.id %></td>
        <td><%= tag_association.created_at %></td>
        <td><%= tag_association.tagable_id %></td>
        <td><%= tag_association.tag_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
use project/:project_id/tag_associations to create tag_associations
